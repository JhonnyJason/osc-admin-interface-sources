//- settings structure
#settings
    .settings-group
        #settingsbutton-account.settings-entry.click-button.icon-entry
            .icon: svg: use(href="#svg-account-icon")
            .text Account
            .arrow: svg: use(href="#svg-chevron-right-icon") 
    
    #settingsgroup-server.grouping-head Server Settings
    .settings-group
        #settingsbutton-signallingserver.settings-entry.click-button 
            .text Signalling Server
            .arrow: svg: use(href="#svg-chevron-right-icon") 
        #settingsbutton-turnserver.settings-entry.click-button 
            .text STUN/TURN Server
            .arrow: svg: use(href="#svg-chevron-right-icon") 

